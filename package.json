{
  "name": "archibnb",
  "version": "1.0.0",
  "description": "ArchiBnb is a clone of the popular home-booking website, AirBnb. It was built as a week-long solo-project for a rigerous 1000+ hour software engineering bootcamp.",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "npm run build --prefix frontend",
    "install": "npm --prefix backend install backend && npm --prefix frontend install frontend",
    "dev:backend": "npm install --prefix backend start",
    "dev:frontend": "npm install --prefix frontend start",
    "sequelize": "npm run --prefix backend sequelize",
    "sequelize-cli": "npm run --prefix backend sequelize-cli",
    "start": "npm start --prefix backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmthorn/archibnb.git"
  },
